{
  "name": "mission-control-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && npx wrangler pages deploy dist --project-name=mission-control-cole",
    "prebuild": "python3 scripts/gen-skills.py && node scripts/gen-crons.js && node scripts/gen-sessions.js",
    "test": "playwright test",
    "test:percy": "npx percy exec -- node tests/percy-full-suite.js",
    "test:percy:local": "BASE_URL=http://localhost:5173 node tests/percy-full-suite.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "better-sqlite3": "^12.6.2",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.563.0",
    "playwright": "^1.58.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@percy/cli": "^1.31.8",
    "@percy/playwright": "^1.0.10",
    "@playwright/test": "^1.58.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
